@page
@model index
@{
    Layout = "Shared/_Layout";
}


<h1>Categorias</h1>
<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Title</td>
            <td>Price</td>
        </tr>
    </thead>
    <tbody>

        @foreach (var category in Model.Categories)
        {
            <tr>
                <td>@category.Id</td>
                <td>@category.Title</td>
                <td>@category.Price</td>
            </tr>
        }
    </tbody>
</table>